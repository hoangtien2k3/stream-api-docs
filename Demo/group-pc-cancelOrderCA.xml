<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xs1="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="cancelOrderCaQueue" class="com.viettel.fw.concurrent Queue">
        <property name="uniqueName" value="cancelOrderCaqueue" />
        <property name="capacity" value="1000"/>
        <property name="joinsysoverLoad" value="true"/>
    </bean>

    <bean id="cancelOrderCaGroup" class="com.viettel,fw.concurrent.POGroup" init-method="init">
        <property name="numofProducer" value="1"/>
        <property name="numofConsumer" value="3"/>
        <property name="cronExpression" value="0 0 * * * ?"/>
        ＜！--<property name="producensteepInterval" value=50000"/>-->
        <property name="producerBatchSize" value="100"/>
        <property name="queue" ref="cancelOrderCaQueue"/>
        <property name="clazzProducer" value="com viettel,bocs.orden.po.CancelOrderCAProducer" />
        <property name="clazzConsumer" value="com.viettel.bocs.order.pc.CancelOrderCAConsumer"/>

        <property name="contextParamMap">
            <map>
                <entry key="minOfDay" value="2023-09-01 00:00:00"/>
            </map>
        </property>
    </bean>

</beans>